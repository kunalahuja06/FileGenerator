<UserControl x:Class="FileGenerator.PatternControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             >
    <Grid>
        <ListBox SelectionChanged="ListBox_SelectionChanged" Loaded="UserControl_Loaded">
            <ListBoxItem Content="Repository Pattern" x:Uid="RepositoryPattern" Padding="5,10,5,10" VerticalAlignment="Center" />
            <ListBoxItem Content="Unit of Work Pattern" x:Uid="UnitOfWorkPattern" Padding="5,10,5,10" VerticalAlignment="Center" />
        </ListBox>
    </Grid>
</UserControl>
